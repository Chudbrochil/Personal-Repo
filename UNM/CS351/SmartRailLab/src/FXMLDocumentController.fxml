<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <Canvas fx:id="canvasRail" height="600.0" onMouseClicked="#canvasClicked" width="800.0" GridPane.columnSpan="2" GridPane.rowIndex="1" />
      <TextField fx:id="tfOutput" editable="false" />
      <Button mnemonicParsing="false" text="Make Train" GridPane.columnIndex="1" onAction="#makeTrain" />
   </children>
   <columnConstraints>
      <ColumnConstraints maxWidth="713.0" minWidth="155.0" prefWidth="634.0" />
      <ColumnConstraints maxWidth="635.0" minWidth="77.0" prefWidth="156.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
